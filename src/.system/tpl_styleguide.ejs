<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <title>Styleguide for <%= projectName %></title>
    <link rel="stylesheet" href="./default.css">
    <style><%- codeStyle %><%- tmplStyle %><%- css %></style>
  </head>
  <body>
    <header class="masthead">
    <h1><%= projectName %></h1>
    <p class="lead">Generated by <a href="https://github.com/morishitter/postcss-style-guide">postcss-style-guide</a></p>
    </header>
    <% maps.forEach(function (map) { %>
    <div class="section">
        <div class="col"><%- map.html %></div>
        <div class="col"><pre class="code"><code class="lang-css"><%- map.rule %></code></pre></div>
    </div>
    <% })%>
  </body>
</html>
